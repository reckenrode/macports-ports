destroot {
    set docpath ${destroot}${prefix}/share/doc/${name}/
    xinstall -d -m 755 ${docpath}
    xinstall -m 644 -W ${worksrcpath} \
        CONTRIBUTING.md \
        LICENSE \
        README.md \
        ${docpath}
    xinstall -m 755 ${worksrcpath}/${name}/target/[cargo.rust_platform]/release/${name} \
        ${destroot}${prefix}/bin/
}

