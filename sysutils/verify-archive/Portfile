    set docpath ${destroot}${prefix}/share/doc/${name}/
    xinstall -d -m 755 ${docpath}
    xinstall -m 644 -W ${worksrcpath} \
        COPYING \
        README.md \
        ${docpath}
    xinstall -m 755 ${worksrcpath}/target/[cargo.rust_platform]/release/${name} \
        ${destroot}${prefix}/bin/
}

